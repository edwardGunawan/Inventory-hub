{
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "dependencies": {
    "global-prefix": "^1.0.1",
    "is-windows": "^1.0.1",
    "resolve-dir": "^1.0.0"
  },
  "description": "The directory used by npm for globally installed npm modules.",
  "devDependencies": {
    "gulp-format-md": "^0.1.12",
    "mocha": "^3.4.2"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/jonschlinkert/global-modules",
  "license": "MIT",
  "main": "index.js",
  "name": "global-modules",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonschlinkert/global-modules.git"
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "homedir-polyfill",
        "git-config-path",
        "global-prefix",
        "npm-paths"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  },
  "version": "1.0.0"
}