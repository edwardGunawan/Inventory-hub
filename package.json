{
  "name": "inventory-hub",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "dependencies": {
    "concurrently": "^3.5.1",
    "electron": "^1.8.4",
    "glob": "^7.1.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "wait-on": "^2.1.0"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "electron-dev": "DEV_URL=http://localhost:3000 electron .",
    "start": "concurrently \"npm run react-start\" \"wait-on http://localhost:3000 && npm run electron-dev\"",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "devtron": "^1.4.0"
  }
}
